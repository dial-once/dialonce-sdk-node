{
  "name": "dialonce-sdk-node",
  "description": "A Dialonce helper library",
  "keywords": [
    "dialonce",
    "dialonce client"
  ],
  "version": "1.0.0",
  "author": "Yacine Khatal <khatal.yacine@gmail.com>",
  "contributors": [
    {
      "name": "dialonce-sdk-node contributors",
      "url": "https://github.com/dial-once/dialonce-sdk-node/graphs/contributors"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dial-once/dialonce-sdk-node.git"
  },
  "dependencies": {
    "request": "^2.72.0"
  },
  "devDependencies": {
    "istanbul": "^0.4.5",
    "jsdoc": "^3.4.0",
    "jshint": "^2.9.2",
    "minami": "^1.1.1",
    "mocha": "^2.5.3"
  },
  "scripts": {
    "test": "node_modules/jshint/bin/jshint . && node_modules/mocha/bin/mocha --recursive test --timeout=10000",
