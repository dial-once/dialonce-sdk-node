{
  "name": "dialonce-sdk-node",
  "description": "A Dialonce helper library",
  "keywords": [
    "dialonce",
    "dialonce client"
  ],
  "version": "1.0.0",
  "author": "Yacine Khatal <khatal.yacine@gmail.com>",
  "contributors": [
    {
      "name": "dialonce-sdk-node contributors",
      "url": "https://github.com/dial-once/dialonce-sdk-node/graphs/contributors"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dial-once/dialonce-sdk-node.git"
  },
  "dependencies": {
    "request": "^2.72.0"
  },
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "jsdoc": "^3.4.0",
    "minami": "^1.1.1"
  },
  "scripts": {
    "start": "node index.js",
    "lint": "./node_modules/.bin/eslint .",
    "cover": "CONSOLE_LOGGING=false ./node_modules/.bin/nyc --reporter=lcov ./node_modules/.bin/_mocha test --recursive --timeout=10000 --exit",
    "test": "npm run lint && npm run cover",
    "doc": "node_modules/jsdoc/jsdoc.js -d ./docs --readme README.md --package package.json --template node_modules/minami lib/*"
  },
  "main": "./lib",
  "engines": {
    "node": ">=0.12.0"
  },
  "license": "MIT"
}
